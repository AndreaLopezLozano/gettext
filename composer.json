{
    "name": "anavel/gettext",
    "description": "Manage laravel translation files from your admin panel",
    "keywords": [
        "laravel",
        "admin",
        "gettext",
        "cms"
    ],
    "authors": [
        {
            "name": "Adrian P. Blunier",
            "email": "adrian@anavallasuiza.com"
        },
        {
            "name": "Carlos Morales",
            "email": "carlos@anavallasuiza.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*",
        "illuminate/translation": "5.1.*",
        "anavel/foundation": "0.1.*",
        "eusonlito/laravel-gettext": "1.*"
    },
    "require-dev": {
        "orchestra/testbench": "~3.1",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*",
        "whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "Anavel\\Gettext\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anavel\\Gettext\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable"
}
